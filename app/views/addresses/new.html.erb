<div class="container">
  <%= simple_form_for @address do |f|%>
    <%= f.input :zipcode %>
    <%= f.input :country, as: :string %>
    <%= f.input :city %>
    <%= f.input :street%>
    <%= f.input :number %>

    <div class="form-actions">
      <%= f.button :submit, "Submit" %>
    </div>

  <% end %>
</div>
